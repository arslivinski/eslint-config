{
  "name": "@arslivinski/eslint-config",
  "version": "1.0.0-alpha.13",
  "description": "Personal set of ESLint configurations",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "test": "npm run -s test:base && npm run -s test:react && npm run -s test:typescript",
    "test:base": "eslint-find-rules --unused index.js",
    "test:react": "eslint-find-rules --unused react.js --no-core",
    "test:typescript": "eslint-find-rules --unused typescript.js --no-core",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "typescript": "^4.9.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.11",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^45.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^4.9.4"
  },
  "author": {
    "name": "Anderson Roberto Slivinski",
    "url": "https://github.com/arslivinski"
  },
  "homepage": "https://github.com/arslivinski/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arslivinski/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/arslivinski/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "eslintConfig": {
    "ignorePatterns": [
      "!.*.js",
      "node_modules/"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    },
    "extends": [
      "./index.js"
    ],
    "rules": {
      "import/no-commonjs": "off",
      "unicorn/prefer-module": "off"
    }
  }
}
